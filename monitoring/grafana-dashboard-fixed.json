{
  "dashboard": {
    "title": "ML Model Adaptive Routing",
    "tags": ["ml", "adaptive-routing", "epsilon-greedy"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 2,
    "refresh": "5s",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 0},
        "type": "timeseries",
        "title": "Traffic Distribution (Requests/sec)",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "bf54h6xht569sd"
            },
            "expr": "sum by (model_name) (rate(model_requests_total{status=\"success\"}[2m]))",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "calcs": ["lastNotNull", "mean"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "showPoints": "never",
              "lineWidth": 2
            },
            "unit": "reqps",
            "decimals": 2
          }
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 0},
        "type": "timeseries",
        "title": "Model Latency Comparison (seconds)",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "bf54h6xht569sd"
            },
            "expr": "sum by (model_name) (rate(model_request_duration_seconds_sum{status=\"success\"}[2m])) / sum by (model_name) (rate(model_request_duration_seconds_count{status=\"success\"}[2m]))",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "calcs": ["lastNotNull", "mean"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "asc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "showPoints": "never",
              "lineWidth": 2
            },
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 9},
        "type": "bargauge",
        "title": "Total Requests Per Model",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "bf54h6xht569sd"
            },
            "expr": "sum by (model_name) (model_requests_total{status=\"success\"})",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "showUnfilled": true,
          "minVizWidth": 0,
          "minVizHeight": 10
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 9},
        "type": "stat",
        "title": "Current Request Rates",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "bf54h6xht569sd"
            },
            "expr": "sum by (model_name) (rate(model_requests_total{status=\"success\"}[2m]))",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "unit": "reqps",
            "decimals": 2
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 7, "w": 24, "x": 0, "y": 17},
        "type": "table",
        "title": "Model Performance Summary",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "bf54h6xht569sd"
            },
            "expr": "sum by (model_name) (model_requests_total{status=\"success\"})",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "model_name": "Model Name",
                "Value": "Total Requests"
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "footer": {
            "show": false
          }
        }
      }
    ]
  },
  "overwrite": true
}
